{
  "id": "page",
  "label": "Page",
  "format": "page",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": { "label": "UID", "placeholder": "URL slug for the page" }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "Title for the page",
          "allowTargetBlank": false,
          "single": "heading1"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "hero_slice": { "type": "SharedSlice" },
            "model_card": { "type": "SharedSlice" },
            "text_and_media": { "type": "SharedSlice" },
            "card": { "type": "SharedSlice" },
            "carousel": { "type": "SharedSlice" },
            "post_cards_preview": { "type": "SharedSlice" },
            "icons_stack": { "type": "SharedSlice" },
            "title_block": { "type": "SharedSlice" },
            "tabbed_media_block": { "type": "SharedSlice" },
            "anchor_link_section": { "type": "SharedSlice" },
            "accordian_section": { "type": "SharedSlice" },
            "location_cards": { "type": "SharedSlice" },
            "text_block": { "type": "SharedSlice" },
            "image_block": { "type": "SharedSlice" },
            "team_members": { "type": "SharedSlice" },
            "hubspot_form_embed": { "type": "SharedSlice" },
            "community_card": { "type": "SharedSlice" }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_description": {
        "type": "Text",
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        }
      },
      "meta_image": {
        "type": "Image",
        "config": {
          "label": "Meta Image",
          "constraint": { "width": 2400, "height": 1260 },
          "thumbnails": []
        }
      },
      "meta_title": {
        "type": "Text",
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        }
      }
    }
  }
}
